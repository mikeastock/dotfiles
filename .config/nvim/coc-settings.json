{
  "ignore_all_lsp": false,
  "coc.preferences.formatOnSaveFiletypes": ["typescript"],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    }
  },
  "_oldConfig": {
    "languageserver": {
      "elmLS": {
        "command": "elm-language-server",
        "filetypes": ["elm"],
        "rootPatterns": ["elm.json"]
      }
    },
    "codeLens.enable": true
  }
}
